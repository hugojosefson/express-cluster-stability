{
  "name": "express-cluster-stability",
  "version": "1.4.2",
  "description": "Stability and multi-core performance for your Express app, via the built-in stable Node.js cluster API.",
  "main": "./cjs/index.js",
  "module": "./src/index.mjs",
  "files": [
    "src",
    "cjs",
    "package.json",
    "yarn.lock",
    "README.md"
  ],
  "scripts": {
    "start": "src/cli.mjs",
    "prepare": "npm-run-all build-cjs lint-readme",
    "build-cjs": "rollup src/index.mjs --dir cjs --preserveModules --entryFileNames '[name].js' --format cjs --exports named --outro 'if(exports.default){Object.assign(exports.default,exports);module.exports=exports.default}'",
    "lint": "prettier --write '{src,test,example}/**/*.{js,mjs,md}' './*.{js,mjs,md}'",
    "lint-readme": "prettier --write README.md",
    "test": "npm-run-all lint lint-readme mocha",
    "mocha": "mocha",
    "npm-audit": "npm install --package-lock-only && npm audit; rm package-lock.json"
  },
  "author": "Hugo Josefson <hugo@josefson.org> (https://www.hugojosefson.com/)",
  "repository": "hugojosefson/express-cluster-stability",
  "license": "MIT",
  "keywords": [
    "cluster",
    "express",
    "stability",
    "stable",
    "uptime",
    "master",
    "worker",
    "node",
    "processes"
  ],
  "mocha": {
    "spec": "test/*-test.mjs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{mjs,js,jsx}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "express": "^4.17.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "mocha": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rollup": "^2.13.1"
  },
  "engines": {
    "node": ">=13.2.0"
  }
}
